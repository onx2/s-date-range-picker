{
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "plugins": [
    "svelte3"
  ],
  "extends": [
    "eslint:recommended"
  ],
  "overrides": [
    {
      "files": [
        "src/date-range-picker/**/*.svelte",
        "src/date-range-picker/**/*.js"
      ],
      "processor": "svelte3/svelte3"
    }
  ]
}
