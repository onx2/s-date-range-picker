{
  "name": "s-date-range-picker",
  "version": "0.2.0",
  "description": "A date range picker built with Svelte",
  "keywords": [
    "svelte",
    "rollup",
    "date-picker",
    "date-range-picker",
    "datepicker",
    "daterangepicker",
    "date-time-picker",
    "datetimepicker",
    "svelte-components"
  ],
  "browserslist": [
    "defaults"
  ],
  "private": false,
  "main": "build/js/main.js",
  "module": "build/js/main.mjs",
  "svelte": "./src/date-range-picker/index.js",
  "license": "MIT",
  "author": "Jeff Rooks <rooksjj@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/onx2/svelte-date-range-picker"
  },
  "scripts": {
    "format": "npx prettier --write src/date-range-picker\"{,!(node_modules)/**/}*.{js,svelte}\"",
    "lint": "eslint --fix src/date-range-picker\"{,!(node_modules)/**/}*.{js,svelte}\"",
    "cleanup": "rimraf ./build",
    "test": "cross-env NODE_ENV=test jest",
    "prepublishOnly": "yarn format && yarn lint && yarn build",
    "serve": "yarn cleanup && cross-env NODE_ENV=development rollup --config --watch",
    "build": "yarn cleanup && cross-env NODE_ENV=production rollup --config"
  },
  "dependencies": {
    "date-fns": "2.8.1"
  },
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@types/jest": "24.0.23",
    "babel-jest": "24.9.0",
    "chokidar": "3.3.0",
    "cross-env": "6.0.3",
    "deasync": "0.1.16",
    "eslint": "^6.7.2",
    "eslint-plugin-svelte3": "^2.7.3",
    "husky": "^3.1.0",
    "jest": "24.9.0",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "rimraf": "3.0.0",
    "rollup": "1.27.8",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-bundle-html": "0.2.2",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-livereload": "1.0.4",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-serve": "1.0.1",
    "rollup-plugin-svelte": "5.1.1",
    "rollup-plugin-terser": "5.1.2",
    "rollup-plugin-typescript2": "0.25.3",
    "svelte": "3.16.0",
    "svelte-ts-preprocess": "1.1.3",
    "ts-jest": "24.2.0",
    "typescript": "3.7.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.{js,ts}",
      "!**/*.d.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.ts$": "ts-jest",
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": "./svelte-jest.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node",
      "mjs",
      "svelte"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn lint"
    }
  }
}
