{
  "name": "s-date-range-picker",
  "version": "0.1.8",
  "description": "A date range picker built with Svelte",
  "keywords": [
    "svelte",
    "rollup",
    "date-picker",
    "date-range-picker",
    "datepicker",
    "daterangepicker",
    "date-time-picker",
    "datetimepicker",
    "svelte-components"
  ],
  "private": false,
  "main": "build/js/main.js",
  "module": "build/js/main.mjs",
  "iife": "build/js/main.iife.js",
  "svelte": "./src/date-range-picker/index.js",
  "license": "MIT",
  "author": "Jeff Rooks <rooksjj@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/onx2/svelte-date-range-picker"
  },
  "scripts": {
    "cleanup": "rimraf ./build",
    "test": "cross-env NODE_ENV=test jest",
    "prepublishOnly": "yarn build",
    "serve": "yarn cleanup && cross-env NODE_ENV=development rollup --config --watch",
    "build": "yarn cleanup && cross-env NODE_ENV=production rollup --config"
  },
  "dependencies": {
    "date-fns": "2.8.1"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@types/jest": "24.0.15",
    "babel-jest": "24.8.0",
    "chokidar": "3.0.1",
    "cross-env": "5.2.0",
    "deasync": "0.1.15",
    "jest": "24.8.0",
    "rimraf": "2.6.3",
    "rollup": "1.16.1",
    "rollup-plugin-bundle-html": "0.2.1",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-livereload": "1.0.1",
    "rollup-plugin-node-resolve": "5.0.4",
    "rollup-plugin-serve": "1.0.1",
    "rollup-plugin-svelte": "5.1.0",
    "rollup-plugin-terser": "5.0.0",
    "rollup-plugin-typescript2": "0.21.2",
    "svelte": "3.5.3",
    "svelte-ts-preprocess": "1.1.3",
    "ts-jest": "24.0.2",
    "typescript": "3.5.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/**/*.{js,ts}",
      "!**/*.d.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.ts$": "ts-jest",
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": "./svelte-jest.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node",
      "mjs",
      "svelte"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    }
  }
}
